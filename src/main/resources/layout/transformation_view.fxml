<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pl.yalgrin.gremphics.controller.TransformationViewController">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" BorderPane.alignment="CENTER">
            <top>
                <FlowPane maxHeight="-Infinity" minHeight="-Infinity" prefWidth="200.0" BorderPane.alignment="CENTER">
                    <children>
                        <Button fx:id="newShapeButton" layoutX="84.0" layoutY="15.0" mnemonicParsing="false"
                                onAction="#newShapePressed" text="New shape">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="deleteShapeButton" layoutX="167.0" layoutY="15.0" mnemonicParsing="false"
                                onAction="#deleteShapePressed" text="Delete shape">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="selectionModeButton" layoutX="195.0" layoutY="15.0" mnemonicParsing="false"
                                onAction="#selectModePressed" text="Selection mode">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="addPointButton" layoutX="195.0" layoutY="15.0" mnemonicParsing="false"
                                onAction="#addPointPressed" text="Add point mode">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="translateModeButton" layoutX="479.0" layoutY="15.0" mnemonicParsing="false"
                                onAction="#translateModePressed" text="Translation mode">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="rotateModeButton" mnemonicParsing="false" onAction="#rotateModePrressed"
                                text="Rotation mode">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="scaleModeButton" layoutX="84.0" layoutY="15.0" mnemonicParsing="false"
                                onAction="#scaleModePressed" text="Scaling mode">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="translateDialogButton" mnemonicParsing="false" onAction="#translateDialogPressed"
                                text="Translation dialog">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="rotateDialogButton" mnemonicParsing="false" onAction="#rotateDialogPressed"
                                text="Rotation dialog">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                        <Button fx:id="scaleDialogPressed" mnemonicParsing="false" onAction="#scaleDialogPressed"
                                text="Scale dialog">
                            <FlowPane.margin>
                                <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                            </FlowPane.margin>
                        </Button>
                    </children>
                </FlowPane>
            </top>
            <center>
                <SplitPane orientation="VERTICAL" prefHeight="200.0" prefWidth="160.0" BorderPane.alignment="CENTER">
                    <items>
                        <AnchorPane fx:id="centerPane" minHeight="200.0" minWidth="200.0" prefHeight="200.0"
                                    prefWidth="200.0"/>
                    </items>
                </SplitPane>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
