<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.layout.*?>
<AnchorPane maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="600.0" prefWidth="800.0"
            xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="pl.yalgrin.gremphics.controller.EditorViewController">
    <children>
        <BorderPane prefHeight="200.0" prefWidth="200.0" AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                    AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0" BorderPane.alignment="CENTER">
            <center>
                <SplitPane dividerPositions="0.8" prefHeight="160.0" prefWidth="300.0" BorderPane.alignment="CENTER">
                    <items>
                        <AnchorPane fx:id="centerPane" minHeight="200.0" minWidth="200.0" prefHeight="200.0"
                                    prefWidth="200.0"/>
                        <AnchorPane>
                            <children>
                                <FlowPane AnchorPane.bottomAnchor="0.0" AnchorPane.leftAnchor="0.0"
                                          AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
                                    <children>
                                        <FlowPane fx:id="buttonPane" orientation="VERTICAL" prefHeight="300.0">
                                            <children>
                                                <Button fx:id="selectButton" contentDisplay="CENTER"
                                                        mnemonicParsing="false" text="Select" textAlignment="CENTER">
                                                    <FlowPane.margin>
                                                        <Insets bottom="5.0" left="5.0" right="5.0" top="5.0"/>
                                                    </FlowPane.margin>
                                                </Button>
                                            </children>
                                        </FlowPane>
                                        <FlowPane fx:id="propertyPane" prefHeight="200.0" prefWidth="200.0"/>
                                    </children>
                                </FlowPane>
                            </children>
                        </AnchorPane>
                    </items>
                </SplitPane>
            </center>
        </BorderPane>
    </children>
</AnchorPane>
